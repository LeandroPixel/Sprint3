@{
	ViewBag.Title = HttpContext.Current.Application["Site_Name"] + " | Checkout";

	ViewBag.MinCSS = HttpContext.Current.Application["MIN_CSS"].ToString() == "1" ? ".min" : "";
	ViewBag.MinJS = HttpContext.Current.Application["MIN_JS"].ToString() == "1" ? ".min" : "";
	ViewBag.VersaoCSS = HttpContext.Current.Application["VERSAO_CSS"].ToString();
	ViewBag.VersaoJS = HttpContext.Current.Application["VERSAO_JS"].ToString();
	ViewBag.MinCSS_FRAMEWORK = HttpContext.Current.Application["MIN_CSS_FRAMEWORK"].ToString() == "1" ? ".min" : "";
	ViewBag.MinJS_FRAMEWORK = HttpContext.Current.Application["MIN_JS_FRAMEWORK"].ToString() == "1" ? ".min" : "";

	ViewBag.Breadcrumb = new Dictionary<string, string>();
	ViewBag.Breadcrumb.Add("Painel Administrativo", Url.Action("Index", "Painel"));
    ViewBag.Breadcrumb.Add("Produtos", "javascript:oCheckout.GoToProducts('" + Url.Action("Index", "Produtos") + "','"+Url.Action("Produtos", "Offline")+"')");
	ViewBag.Breadcrumb.Add("Carrinho", "");

	Layout = "~/Views/Shared/_Layout.cshtml";

	int QtdMaxParcelasCartao = 12;
	int QtdMaxParcelasBoleto = 12;
	int QtdMaxParcelasCheque = 12;
    int QtdMaxPeriodos = 12;
	string Url_Image_Server = HttpContext.Current.Application["IMAGES_SERVER_URL_HTTP"].ToString();
}
@section CSSHeader{
	<link rel="stylesheet" href="~/Content/css/framework/jquery.datetimepicker@(ViewBag.MinCSS_FRAMEWORK).css" />
	<link rel="stylesheet" href="~/Content/css/checkout@(ViewBag.MinCSS).css?v=@(ViewBag.VersaoCSS)" />
}
@section ScriptHeader{
    <script>
        var cookie_user_name = "@System.Web.HttpContext.Current.Application["Cookie_User_Offline"].ToString()"; 
    </script>    
	<script type="text/javascript" src="~/Content/js/framework/jquery.datetimepicker.full@(ViewBag.MinJS_FRAMEWORK).js"></script>
	<script type="text/javascript" src="~/Content/js/checkout_classes@(ViewBag.MinJS).js?v=@(ViewBag.VersaoJS)"></script>
	<script type="text/javascript" src="~/Content/js/checkout@(ViewBag.MinJS).js?v=@(ViewBag.VersaoJS)"></script>

}

<section id="section-principal">
	<form method="post" id="formPedido">
		<div class="conteudo-geral mdl-grid">
			<div class="mdl-cell--12-col">
				<div class="selos-informativos text-center">
					<a href="http://www.google.com/safebrowsing/diagnostic?site=@(HttpContext.Current.Request.Url.GetLeftPart(UriPartial.Authority).ToString() + System.Web.HttpContext.Current.Request.ApplicationPath)" target="_blank">
						<img src="~/Content/images/selo_navegacao-segura.png" title="Navegação segura" alt="Selo navegação segura Google Safe" class="" />
					</a>
					<img src="~/Content/images/selo_compra-segura.png" title="Compra segura" alt="Selo compra segura" class="" />
					<img src="~/Content/images/selo_satisfacao.png" title="Satisfação garantida" alt="Selo satisfação garantida" class="" />
				</div>
			</div>
			<div class="mdl-cell--12-col">
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-resumo">Dados do pedido</label>
				<div class="mdl-cell--12-col">
					<!-- TABELA DE PRODUTOS -->
					<table class="table table-striped" id="tb_produtos">
						<thead>
							<tr>
								<th>&nbsp;</th>
								<th>Produto</th>
								<th class="text-center">Quantidade</th>
								<th class="text-center">Fotos selecionadas</th>
							</tr>
						</thead>
						<tbody>
						</tbody>
					</table>
					<input type="hidden" class="" id="dadosPedidoValorTotal" value="" />
					<!-- /TABELA DE PRODUTOS -->
				</div>
			</div>
			<div class="mdl-cell--6-col">
				<!-- CPF PARA NF-e -->
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-nfe" for="dadosPedidoNfeCpfCnpj">CPF / CNPJ</label>
				<div class="mdl-cell--12-col mdl-textfield mdl-js-textfield div-dados-pedido-nfe-cpf-cnpj">
					<input class="mdl-textfield__input" type="text" pattern="[0-9]*(\.[0-9]+)?" maxlength="14" title="Somente números" id="dadosPedidoNfeCpfCnpj" name="dadosPedidoNfeCpfCnpj" />
					<label class="mdl-textfield__label" for="dadosPedidoNfeCpfCnpj">Digite seu CPF/CNPJ</label>
					<span class="mdl-textfield__error span-msg-cpf-cnpj">Favor informar CPF ou CNPJ válido (somente números)</span>
				</div>
				<!-- /CPF PARA NF-e -->
			</div>
			<div class="mdl-cell--6-col">
				<!-- ENDEREÇO PARA ENTREGA -->
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-endereco" for="">Dados de entrega</label>
				<div class="mdl-cell--12-col font_weight-300 mdl-grid padding-0">
					<div class="mdl-cell--10-col">
						<p class="dados-pedido-endereco dados-pedido-endereco-destinatario"></p>
						<p class="dados-pedido-endereco dados-pedido-endereco-completo"></p>
						<p class="dados-pedido-endereco dados-pedido-endereco-cep"></p>
							
						<input type="hidden" name="dadosPedidoEnderecoCEP" class="hiddenPedidoEnderecoCEP" value="" />
						<input type="hidden" name="dadosPedidoEnderecoDestinatario" class="hiddenPedidoEnderecoDestinatario" value="" />
						<input type="hidden" name="dadosPedidoEnderecoLogradouro" class="hiddenPedidoEnderecoLogradouro" value="" />
						<input type="hidden" name="dadosPedidoEnderecoNumero" class="hiddenPedidoEnderecoNumero" value="" />
						<input type="hidden" name="dadosPedidoEnderecoComplemento" class="hiddenPedidoEnderecoComplemento" value="" />
						<input type="hidden" name="dadosPedidoEnderecoBairro" class="hiddenPedidoEnderecoBairro" value="" />
						<input type="hidden" name="dadosPedidoEnderecoMunicipio" class="hiddenPedidoEnderecoMunicipio" value="" />
						<input type="hidden" name="dadosPedidoEnderecoEstado" class="hiddenPedidoEnderecoEstado" value="" />
					</div>
					<div class="mdl-cell--2-col">
						<button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect btn-alterar-endereco-entrega">Alterar</button>
						@*<button type="button" class="btn btn-secondary btn-alterar-endereco-entrega">Alterar</button>*@
					</div>
				</div>
				<!-- /ENDEREÇO PARA ENTREGA -->
			</div>
            <div class="mdl-cell--6-col">
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-endereco" for="">Observação</label>
				<div class="mdl-cell--12-col font_weight-300 mdl-grid padding-0">
                    <p>
                        <textarea maxlength="250" cols="60" rows="5" id="txtObservacao"></textarea>
                    </p>
                </div>
            </div>
			<div class="mdl-cell--6-col">
				<!-- DADOS COMPRA -->
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-endereco" for="">Dados do Comprador:</label>
				<div class="mdl-cell--12-col font_weight-300 mdl-grid padding-0">
					<div class="mdl-cell--10-col">
						<p class="dados-pedido-endereco dados-pedido-comprador"></p>
						<p class="dados-pedido-endereco dados-pedido-comprador-completo"></p>
						<p class="dados-pedido-endereco dados-pedido-comprador-cep"></p>

						<input type="hidden" name="dadosPedidoCompradorCpfCnpj" class="hiddenPedidoCompradorCpfCnpj" value="" />
                        <input type="hidden" name="dadosPedidoCompradorNome" class="hiddenPedidoCompradorNome" value="" />
                        <input type="hidden" name="dadosPedidoCompradorDDD" class="hiddenPedidoCompradorDDD" value="" />
                        <input type="hidden" name="dadosPedidoCompradorTelefone" class="hiddenPedidoCompradorTelefone" value="" />
						<input type="hidden" name="dadosPedidoCompradorEmail" class="hiddenPedidoCompradorEmail" value="" />
							
						<input type="hidden" name="dadosPedidoCompradorCEP" class="hiddenPedidoCompradorCEP" value="" />
						<input type="hidden" name="dadosPedidoCompradorDestinatario" class="hiddenPedidoCompradorDestinatario" value="" />
						<input type="hidden" name="dadosPedidoCompradorLogradouro" class="hiddenPedidoCompradorLogradouro" value="" />
						<input type="hidden" name="dadosPedidoCompradorNumero" class="hiddenPedidoCompradorNumero" value="" />
						<input type="hidden" name="dadosPedidoCompradorComplemento" class="hiddenPedidoCompradorComplemento" value="" />
						<input type="hidden" name="dadosPedidoCompradorBairro" class="hiddenPedidoCompradorBairro" value="" />
						<input type="hidden" name="dadosPedidoCompradorMunicipio" class="hiddenPedidoCompradorMunicipio" value="" />
						<input type="hidden" name="dadosPedidoCompradorEstado" class="hiddenPedidoCompradorEstado" value="" />
                        <input type="hidden" name="dadosPedidoCompradorDataNascimento" class="hiddenPedidoCompradorDataNascimento" value="" />
					</div>
					<div class="mdl-cell--2-col">
                        <button id="btn-alterar-comprador-entrega" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect">Alterar</button>
						@*<button type="button" class="btn btn-secondary btn-alterar-endereco-entrega">Alterar</button>*@
					</div>
				</div>
				<!-- /ENDEREÇO PARA ENTREGA -->
			</div>
			@*<div class="mdl-cell--12-col">
				<!-- FRETE -->
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-frete" for="">Frete e manuseio</label>
				<div class="mdl-cell--12-col font_weight-300">
					<label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="dadosPedidoOpcaoFreteNormal">
						<input type="radio" id="dadosPedidoOpcaoFreteNormal" class="mdl-radio__button" name="dadosPedidoOpcaoFrete" value="n" checked />
						<span class="mdl-radio__label">Frete Normal - R$ 9,80</span>
						<span class="">- Entrega em até 13 dias úteis</span>
					</label>
					<label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="dadosPedidoOpcaoFreteExpresso">
						<input type="radio" id="dadosPedidoOpcaoFreteExpresso" class="mdl-radio__button" name="dadosPedidoOpcaoFrete" value="e" />
						<span class="mdl-radio__label">Frete Expresso - R$ 14,50</span>
						<span class="">- Entrega em até 13 dias úteis</span>
					</label>
				</div>
				<!-- /FRETE -->
			</div>*@

            <div class="mdl-cell--12-col">
				<label class="mdl-cell--12-col mdl-typography--title label-dados-pedido label-dados-pedido-endereco" for="">Opções de pagamento</label>
				<div class="font_weight-300 mdl-grid padding-0 dvBoxItensTotalPrazo">
                    Valor Total
                    <p>
                        <input type="text" id="txtTotalVendedor" placeholder="0,00" pattern="[0-9]*(\.[0-9]+)?" title="Somente números"/>
                    </p>
                </div>
                <div class="dvBoxItensTotalPrazo">
                    Períodos ( meses )
                    <p>
                        <select id="qtdPeriodos">
							<option value="" selected>Número de períodos</option>
							@{
                                for (int i = 1; i <= QtdMaxPeriodos; i++)
								{
									<option value="@(i.ToString())">@(i.ToString())</option>
								}
							}
                        </select>
                    </p>
                </div>
				<div class="font_weight-300 mdl-grid padding-0 dvBoxItensTotalPrazo">
                    Data Início
                    <p>
                        <input type="text" id="txtDataInicio" disabled/>
                    </p>
                </div>
                <div class="dvBoxItensTotalPrazo">
                    Data Fim
                    <p>
                        <input type="text" id="txtDataFim" disabled />
                    </p>
                </div>
            </div>

