<!-- MODAL ALTERAÇÃO ENDEREÇO -->
<div class="modal fade div-modal-alteracao-entrega" role="dialog" style="font-size:12px;">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h4 class="modal-title jqTituloBoxDados">Dados de entrega</h4>
			</div>
			<div class="modal-body">
				<form id="form-alteracao-endereco-entrega">
                    <input type="hidden" name="tipo_entrega_comprador" class="hidden_tipo_destinatario_comprador" value="" />
					<table class="table-alteracao-endereco-entrega">
						<tbody>
							<tr class="jqComprador">
								<td><label for="">CPF / CNPJ</label></td>
								<td><input type="text" name="" class="tbxCpfCnpj" pattern="[0-9]*(\.[0-9]+)?" title="Somente números" value="" placeholder="" maxlength="14" size="25" /></td>
							</tr>
							<tr class="jqComprador">
								<td><label for="">Nome</label></td>
								<td><input type="text" name="" class="tbxNome" value="" placeholder="Ex.: José da Silva" maxlength="50" size="40" /></td>
							</tr>
							<tr class="jqComprador">
								<td><label for="">DDD</label></td>
								<td><input type="text" name="" class="tbxDDD" value="" placeholder="" maxlength="3" size="10" /></td>
							</tr>
							<tr class="jqComprador">
								<td><label for="">Telefone</label></td>
								<td><input type="text" name="" class="tbxTelefone" value="" placeholder="" maxlength="50" size="40" /></td>
							</tr>
							<tr class="jqComprador">
								<td><label for="">Email</label></td>
								<td><input type="text" name="" class="tbxEmail" value="" placeholder="" maxlength="50" size="40" /></td>
							</tr>
							<tr class="jqComprador">

								<td><label for="">Data Nascimento</label></td>
								<td>
                                    <!--<input type="date" name="" class="tbxDtNascimento" value="" placeholder="" maxlength="50" size="40" />-->

                                    <div id='datePickerDataNascimento' class='div-input-date-picker'> 
                                        <input class='datetimepicker' onfocus='blur();' id="data_nascimento_comprador" type='text'>
                                        <button id="btnDataNascimento" class='mdl-button mdl-js-button mdl-button--icon btn-data-picker'>
                                            <i class='material-icons'>more_horiz</i>
                                        </button>
                                    </div>
								</td>
							</tr>
							<tr>
								<td><label for="">CEP</label></td>
								<td>
									<input type="text" name="" class="tbxCep" value="" placeholder="Somente números" maxlength="8" size="17" />
									<a href="http://www.buscacep.correios.com.br/sistemas/buscacep/buscaCep.cfm" target="_blank" class="btn-alteracao-endereco-entrega-nao-sei-meu-cep" style="color: indigo;">Não sei meu CEP</a>
								</td>
							</tr>
							<tr class="jqEntrega">
								<td><label for="">Destinatário</label></td>
								<td><input type="text" name="" class="tbxDestinatario" value="" placeholder="Ex.: José da Silva" maxlength="50" size="40" /></td>
							</tr>
							<tr>
								<td><label for="">Logradouro</label></td>
								<td><input type="text" name="" class="tbxLogradouro" value="" placeholder="Ex.: Avenida Paulista" maxlength="50" size="40" /></td>
							</tr>
							<tr>
								<td><label for="">Número</label></td>
								<td><input type="text" name="" class="tbxNumero" value="" placeholder="Ex.: 20" maxlength="6" size="10" /></td>
							</tr>
							<tr>
								<td><label for="">Complemento</label></td>
								<td><input type="text" name="" class="tbxComplemento" value="" placeholder="Ex.: APTO 1012" maxlength="30" size="40"/></td>
							</tr>
							<tr>
								<td><label for="">Bairro</label></td>
								<td><input type="text" name="" class="tbxBairro" value="" placeholder="Ex.: Centro" maxlength="40" size="40" /></td>
							</tr>
							<tr>
								<td><label for="">Cidade</label></td>
								<td><input type="text" name="" class="tbxCidade" value="" placeholder="Ex.: Rio de Janeiro" maxlength="40" size="40" /></td>
							</tr>
							<tr>
								<td><label for="">Estado</label></td>
								<td>
									<select name="" class="sbxEstado">
										<option value="" selected>Selecione</option>
										@{
											foreach (var Estado in ((Dictionary<string, string>)ViewBag.Estados).OrderBy(e => e.Value))
											{
												<option value="@Estado.Key">@Estado.Value</option>
											}
										}
									</select>
								</td>
							</tr>
						</tbody>
					</table>
				</form>
			</div>
			<div class="modal-footer">
				<div class="text-center">
					<p class="msg-erro-validacao-alteracao-endereco-entrega" style="display: none;"></p>
				</div>
				<button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
				<button type="button" class="btn btn-primary btn-confirmar-alteracao-endereco-entrega" >Alterar</button>
			</div>
		</div>
	</div>
</div>
<!-- /MODAL ALTERAÇÃO ENDEREÇO -->